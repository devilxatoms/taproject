                                    aria-expanded="false">
                                    <span class="fa fa-caret-down cDark s25"></span>
                                </button>
                                <button
                                    type="button"
                                    class="navbar-toggle collapsed noMargin noBorder bgTransparent"
                                    data-toggle="collapse"
                                    data-target="#collapseModuleMenu"
                                    aria-expanded="false">
                                    <span class="fa fa-chevron-circle-down cDark s25"></span>
                                </button>
                            </div>
                            <div
                                class="collapse navbar-collapse noBorder pull-left noPadding"
                                id="collapseModuleMenu">
                                <component is="uiMenuControl"
                                    v-bind:data-model="menuControl"
                                    class-name="nav nav-tabs"
                                ></component>
                            </div>
                            <div
                                class="collapse navbar-collapse noBorder pull-right collapse-nav "
                                id="collapseMain">
                                <ul class="nav navbar-nav navbar-right bg-uiColor-base noMargin">
                                    <li class="dropdown">
                                        <a
                                            href=""
                                            data-tooltip="Usuario"
                                            role="button"
                                            class="dropdown-toggle has-tooltip"
                                            data-toggle="dropdown"
                                            role="button"
                                            aria-haspopup="true"
                                            aria-expanded="false">
                                            <span class="fa fa-user " ></span>
                                            <span class="hidden-lg hidden-md hidden-sm">Usuario</span>

                                        </a>
                                        <ul class="dropdown-menu noMargin noPadding">
