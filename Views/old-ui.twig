                                                <div class="content contentMiddle text-center">
                                                    <span class="s80 fa fa-user cWhite"></span>
                                                </div>
                                            </li>
                                            <li>
                                               <a role="button">Perfil</a>
                                            </li>
                                            <li>
                                                <a href="#">Cuenta</a>
                                            </li>
                                            <li>
                                                <a href="#">Settings</a>
                                            </li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a
                                            class="has-tooltip"
                                            data-tooltip="Tabs"
                                            role="button">
                                            <span class="ui ui-tab"></span>
                                        </a>
                                    </li>
                                    <li>
                                        <component is="uiLateralControl"
                                            v-bind:data-model="lateralControl"
                                            class-name="dropdown-menu dropdown" >
                                        </component>
                                    </li>
                                    <li>
                                        <a class="has-tooltip" data-tooltip="Notificaciones" role="button">
