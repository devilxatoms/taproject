<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <title>TuAdmin</title>
        {{ include('stylesheets.twig') }}
    </head>
    <body>
        <div id="uiBody" class="ui-body">
            <aside id="main-nav" class="main-menu bg-uiColor-dark expanded">
                <ul class="navigation">
                    <li>
                        <span class="module-icon">
                        </span>
                        <span class="nav-text cWhite">
                            &nbsp;TuAdmin
                        </span>
                    </li>
                    <li class="item">
                        <a @click="openTab('main')" role="button">
                            <span class="module-icon ta ta-icon-ta-01">
                            </span>
                            <span class="nav-text">
                                Escritorio
                            </span>
                        </a>
                    </li>
                    {{ include('sidebar2.twig') }}
                    <li class="item">
