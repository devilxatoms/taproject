                                        <div id="date" class="fc-uiColor-clear text-bold"></div>
                                    </div>
                                    <div class="clockdate-wrapper content-fluid contentMiddle">
                                        <div id="clock" class="fc-uiColor-clear text-bold">

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </nav>
