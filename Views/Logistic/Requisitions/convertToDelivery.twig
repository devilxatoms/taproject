{% block content %}
    <section id="formConvertToDelivery">
        <div class="header-buttons">
           <div class="btn-group main-buttons">
                <a
                    v-if="flagTypeButton"
                    role="button"
                    class="btn btn  btn-success "
                    v-on:click="onConvertToDelivery">
                    <i class="fa fa-refresh" aria-hidden="true"></i>
                    Convertir A Envío</a>
                <a
                    v-else
                    role="button"
                    class="btn btn  btn-success "
                    v-on:click="onConvertToDelivery">
                    <i class="glyphicon glyphicon-floppy-disk" aria-hidden="true"></i>
                    Guardar Envío</a>
            </div>
        </div>
        <div class="panel-content">
            <form>
                <div class="row">
                    <div class="col-lg-12 col-md-5">
                        <div class="box box-default">
                            <div class="box-header">
                                <h3 class="box-title">ASIGNAR FECHA/HORA, CATEGORÍA Y SUBCATEGORÍA</h3>
                            </div>
                            <div class="box-body">
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="requisitionDate">Fecha/Hora De Salida</label>
                                            <el-date-picker
                                                name="pickupDate"
                                                v-model="pickupDate"
                                                type="datetime"
                                                placeholder="Selecciona Fecha/Hora de salida"
                                                :picker-options="pickerOptions">
                                            </el-date-picker>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="deliveryPossibleDate">Fecha/Hora De Llegada</label>
                                            <el-date-picker
                                                name="deliveryPossibleDate"
                                                v-model="deliveryPossibleDate"
                                                type="datetime"
                                                placeholder="Selecciona Fecha/Hora de llegada"
                                                :picker-options="pickerOptions">
                                            </el-date-picker>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="categorySelect">Categoría</label>
                                            <el-select v-on:change="categoryChange($event)" v-model="categorySelect"placeholder="Selecciona una categoría">
                                                <el-option
                                                    v-for="item in categoryData"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                    <span>${ item.name }</span>
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="subcategorySelect">Subcategoría</label>
                                            <el-select v-model="subcategorySelect" name="subcategorySelect" placeholder="Selecciona una Subcategoría">
                                                <el-option
                                                    v-for="item in subcategoryData"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                    <span>${ item.name }</span>
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12 col-md-5">
                        <div class="box box-default">
                            <div class="box-header">
                                <h3 class="box-title">DETALLES Y CONTENIDO DEL ENVÍO</h3>
                                <div class="box-tools pull-right">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse"
                                            data-toggle="tooltip"
                                            title="Minimizar">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                </div>
                            </div>
                            {#INICIA CONTENIDO DEL ENVIO#}
                            <div class="box-body">
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="requisitionDate">Fecha Requisición</label>
                                            <input type="text" class="form-control" v-model="requisitionDate" name="requisitionDate" readonly="true"/>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="deliveryProvider">Proveedor De Envíos</label>
                                            <input type="text" class="form-control" v-model="deliveryProvider" name="deliveryProvider" readonly="true"/>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="shipType">Tipo</label>
                                            <input type="text" class="form-control" v-model="shipType" name="shipType" readonly="true"/>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="areaShipType">Tipo De Envío</label>
                                            <input type="text" class="form-control" v-model="areaShipType" name="areaShipType" readonly="true"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div v-if="hasMovement" class="box-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12">
                                        <div class="table-responsive">
                                            <table class="table table-bordered">
                                                <thead>
                                                    <tr>
                                                        <th class="col-md">Código</th>
                                                        <th class="col-md">Código Barras</th>
                                                        <th class="col-xl">Nombre</th>
                                                        <th class="col-md">Unidad</th>
                                                        <th class="col-md">Cantidad a Salir</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr v-for="item in datasource">
                                                        <td>${ item.code }</td>
                                                        <td>${ item.barcode }</td>
                                                        <td>${ item.name }</td>
                                                        <td>${ item.unit_id }</td>
                                                        <td>${ item.pivot.to_receive }</td> 
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div v-else class="box-body">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12">
                                         <vue-dynamic-table
                                            class-name="table table-bordered"
                                            v-bind:filter-enabled="false"
                                            v-bind:expandable-row="true"
                                            v-bind:columns="columns"
                                            sort-indicator-asc-class="fa fa-caret-up"
                                            sort-indicator-desc-class="fa fa-caret-down"
                                            v-bind:pagination="false"
                                            v-bind:data-model="datasource">

                                        <template slot="no-rows-found">
                                            <span>No hay items en la lista</span>
                                        </template>
                                    </div>
                                </div>
                            </div>
                            {#TERMINA CONTENIDO DEL ENVIO#}
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <div class="box box-default">
                            <div class="box-header">
                                <h3 class="box-title">DATOS DE REMITENTE</h3>
                                <div class="box-tools pull-right">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse"
                                            data-toggle="tooltip"
                                            title="Minimizar">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="box-body">
                                <div v-if="!(flagBtnsPickup)" class="row">
                                    <div class="col-md-12">
                                        <a
                                            v-if="hasMovement"
                                            role="button"
                                            class="btn btn-success"
                                            v-on:click="onEditAddress(1)"
                                            data-panelsize="md">
                                            <i class="glyphicon glyphicon-edit" aria-hidden="true"></i>
                                            Editar Dirección</a>
                                    </div>
                                </div>
                                <div v-if="!hasMovement" class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label>Dirección Registrada</label>
                                            <input type="checkbox" v-model="checkAddrsManualP" value="">
                                        </div>
                                    </div>
                                </div>
                                {# Se muestra al hacer check en dirección registrada INICIO #}
                                <div class="row" v-if="checkAddrsManualP">
                                    <div  class="col-md-6">
                                        <div class="form-group">
                                            <label>Tipo Remitente</label>
                                            <el-select v-on:change="changePickupType" v-model="selectPickupType">
                                                <el-option
                                                    v-for="item in optionsSelectPickupType"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                    <div v-if="showSecondSelectP" class="col-md-6">
                                        <div class="form-group">
                                            <label>${ lblSelectPick }</label>
                                            <el-select v-on:change="changeProvCustWhP" v-model="provCustWhP">
                                                <el-option
                                                    v-for="item in provCustomerWhP"
                                                    :key="item.id"
                                                    :label="(item.business_name ? item.business_name : item.legal_name) ? 
                                                            (item.business_name ? item.business_name : item.legal_name) : 
                                                            item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                </div>
                                <div v-if="showClientSelectP && checkAddrsManualP" class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Tipo de Subcliente</label>
                                            <el-select v-model="selectSubClientTypeP" v-on:change="changeSubClientTypeP">
                                                <el-option
                                                    v-for="item in optionsSelectCustomerType"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Subcliente</label>
                                            <el-select v-model="selectSubClientP" v-on:change="changeSubClientP">
                                                <el-option
                                                    v-for="item in optionsSelectSubClientP"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                </div>
                                <div v-if="showProvWhSelectP && checkAddrsManualP" class="row">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label>Sucursal</label>
                                            <el-select v-model="selectSubProvP" v-on:change="changeSubsidiaryP">
                                                <el-option
                                                    v-for="item in optionsSelectSubClientP"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                </div>
                                {# Se muestra al hacer check en dirección registrada FIN #}

                                <div class="row">
                                    <div v-if="hasMovement" class="col-md-12">
                                        <div class="form-group">
                                            <label for="idSender">${ lblClientWareOrProvPick }</label>
                                            <input type="text" class="form-control" v-model="idSender" name="idSender" readonly="true"/>
                                        </div>
                                    </div> 
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="sender">Remitente</label>
                                            <input type="text" class="form-control" placeholder="Origen del paquete" v-model="sender" name="sender" :readonly="hasMovement"/>
                                        </div>
                                    </div>
                                </div>

                                {# Solo se muestra si es cliente #}
                                <div class="row" v-if="flagIsCustomerPick">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Tipo</label>
                                            <el-select v-on:change="changeSubCustomer(1)" v-model="selectCustomerType">
                                                <el-option
                                                    v-for="item in optionsSelectCustomerType"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Suc/Consig</label>
                                            <el-select v-on:change="changeSubsidConsign(1)" v-model="selectSubsidConsign">
                                                <el-option
                                                    v-for="item in subsidConsignList"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                </div>
                                {# Solo se muestra si es cliente #}

                                {# Solo se muestra si es proveedor #}
                                <div class="row" v-if="flagIsProviderPick">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label>Sucursal</label>
                                            <el-select v-on:change="changeSubsidiaryProv(1)" v-model="selectSubsidiaryProv">
                                                <el-option
                                                    v-for="item in provSubsidList"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                </div>
                                {# Solo se muestra si es proveedor #}
                                
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="zipCodePick">C.P.</label>
                                            <input type="number" v-on:keyup="onChangeAddressPartP" class="form-control" placeholder="Ej. 27018" v-model="zipCodePick" name="zipCodePick" onKeyPress="if(this.value.length==10) return false;" :readonly="flagReadOnlyPickup"/>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="countryPick">Pais</label>
                                            <input type="text" v-on:keyup="onChangeAddressPartP" class="form-control" placeholder="" v-model="countryPick" name="countryPick" :readonly="flagReadOnlyPickup"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="statePick">Estado</label>
                                            <input type="text" v-on:keyup="onChangeAddressPartP" class="form-control" placeholder="Ej. Coahuila" v-model="statePick" name="statePick" :readonly="flagReadOnlyPickup"/>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="municipalityPick">Municipio</label>
                                            <input type="text" v-on:keyup="onChangeAddressPartP" class="form-control" placeholder="Ej. Torreón" v-model="municipalityPick" name="municipalityPick" :readonly="flagReadOnlyPickup"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="streetPick">Calle</label>
                                            <input type="text" v-on:keyup="onChangeAddressPartP" class="form-control" placeholder="Ej. Av.Juárez" v-model="streetPick" name="streetPick" :readonly="flagReadOnlyPickup"/>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="numExtPick">No Ext.</label>
                                            <input type="number" v-on:keyup="onChangeAddressPartP" class="form-control" placeholder="Ej. 443" v-model="numExtPick" name="numExtPick" onKeyPress="if(this.value.length==10) return false;" :readonly="flagReadOnlyPickup"/>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="numIntPick">No Int.</label>
                                            <input type="number" v-on:keyup="onChangeAddressPartP" class="form-control" placeholder="Ej. 9" v-model="numIntPick" name="numIntPick" onKeyPress="if(this.value.length==10) return false;" :readonly="flagReadOnlyPickup"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="colPick">Colonia</label>
                                            <input type="text" v-on:keyup="onChangeAddressPartP" class="form-control" placeholder="Ej. Centro" v-model="colPick" name="colPick" :readonly="flagReadOnlyPickup"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="referencePick">Referencias</label>
                                            <input type="text" class="form-control" v-model="referencePick" name="referencePick" :readonly="flagReadOnlyPickup"/>
                                        </div>
                                    </div>
                                </div>
                                <div v-if="flagBtnsPickup" class="row">
                                    <div class="col-md-12">
                                        <a
                                            role="button"
                                            class="btn btn btn-danger pull-right"
                                            v-on:click="onCancelEditAddress(1)">
                                            <i class="glyphicon glyphicon-remove-circle" aria-hidden="true"></i>
                                            Cancelar</a>
                                        <a
                                            role="button"
                                            class="btn btn btn-success pull-right"
                                            v-on:click="onSaveAddressPickup">
                                            <i class="glyphicon glyphicon-floppy-disk" aria-hidden="true"></i>
                                            Guardar Dirección</a>
                                    </div>
                                </div>
                                <div class="row mTop10">
                                    <div class="col-md-12">
                                        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                                            <div class="panel panel-default">
                                                <div class="panel-heading" role="tab" id="headingOne">
                                                <h4 class="panel-title">
                                                    <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                                        Mapa para detallar ubicación
                                                    </a>
                                                </h4>
                                                </div>
                                                <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                                                    <div class="panel-body">
                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                <div class="form-group">
                                                                    <div id="mapPickupAddress" style="width: 100%; height: 300px; border: 1px solid black; float:right; "></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="box box-default">
                            <div class="box-header">
                                <h3 class="box-title">DATOS DE DESTINATARIO</h3>
                                <div class="box-tools pull-right">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse"
                                            data-toggle="tooltip"
                                            title="Minimizar">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="box-body" id="boxAddressDeliv">
                                <div v-if="!(flagBtnsDelivery)" class="row">
                                    <div class="col-md-12">
                                        <a
                                            v-if="hasMovement"
                                            role="button"
                                            class="btn btn-success"
                                            v-on:click="onEditAddress(2)"
                                            data-panelsize="md">
                                            <i class="glyphicon glyphicon-edit" aria-hidden="true"></i>
                                            Editar Dirección</a>
                                    </div>
                                </div>
                                <div v-if="!hasMovement" class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label>Dirección Registrada</label>
                                            <input type="checkbox" v-model="checkAddrsManualD" value="">
                                        </div>
                                    </div>
                                </div>
                                {# Se muestra al hacer check en dirección registrada INICIO #}
                                <div class="row" v-if="checkAddrsManualD">
                                    <div  class="col-md-6">
                                        <div class="form-group">
                                            <label>Tipo Destinatario</label>
                                            <el-select v-on:change="changeDeliveryType" v-model="selectDeliveryType">
                                                <el-option
                                                    v-for="item in optionsSelectPickupType"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                    <div v-if="showSecondSelectD" class="col-md-6">
                                        <div class="form-group">
                                            <label>${ lblSelectDeliv }</label>
                                            <el-select v-on:change="changeProvCustWhD" v-model="provCustWhD">
                                                <el-option
                                                    v-for="item in provCustomerWhD"
                                                    :key="item.id"
                                                    :label="(item.business_name ? item.business_name : item.legal_name) ? 
                                                            (item.business_name ? item.business_name : item.legal_name) : 
                                                            item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                </div>
                                <div v-if="showClientSelectD && checkAddrsManualD" class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Tipo de Subcliente</label>
                                            <el-select v-model="selectSubClientTypeD" v-on:change="changeSubClientTypeD">
                                                <el-option
                                                    v-for="item in optionsSelectCustomerType"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label>Subcliente</label>
                                            <el-select v-model="selectSubClientD" v-on:change="changeSubClientD">
                                                <el-option
                                                    v-for="item in optionsSelectSubClientD"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                </div>
                                <div v-if="showProvWhSelectD && checkAddrsManualD" class="row">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label>Sucursal</label>
                                            <el-select v-model="selectSubProvD">
                                                <el-option
                                                    v-for="item in optionsSelectSubClientD"
                                                    :key="item.id"
                                                    :label="item.name"
                                                    :value="item.id">
                                                </el-option>
                                            </el-select>
                                        </div>
                                    </div>
                                </div>
                                {# Se muestra al hacer check en dirección registrada FIN #}
                                <div class="row">
                                    <div class="col-md-12">
                                        <div v-if="hasMovement" class="row">
                                            <div class="form-group col-md-12">
                                                <label for="idAddressee">${ lblClientWareOrProvDeliv }</label>
                                                <input type="text" class="form-control" v-model="idAddressee" name="idAddressee" readonly="true"/>
                                            </div>
                                        </div> 
                                        <div class="form-group">
                                            <label for="addressee">Destinatario</label>
                                            <input type="text" class="form-control" placeholder="Destino del paquete" v-model="addressee" name="addressee" :readonly="flagReadOnlyDeliv"/>
                                        </div>

                                        {# Solo se muestra si es cliente #}
                                        <div class="row" v-if="flagIsCustomerDeliv">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Tipo</label>
                                                    <el-select v-on:change="changeSubCustomer(2)" v-model="selectCustomerType">
                                                        <el-option
                                                            v-for="item in optionsSelectCustomerType"
                                                            :key="item.id"
                                                            :label="item.name"
                                                            :value="item.id">
