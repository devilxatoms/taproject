<div id="modalFormLicense" class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title">${ modalTitle }</h4>
            </div>
            <div id="formLicense">
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group req">
                                <label for="numLicense">No. Licencia:</label>
                                <input
                                    type="number"
                                    v-model="numLicense"
                                    name="numLicense"
                                    autocomplete="off">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group req">
                                <label for="licenseType">Tipo De Licencia:</label>
                                <el-select v-model="licenseType" name="licenseType" placeholder="Selecciona un tipo">
                                    <el-option
                                        v-for="item in dataLicenseType"
                                        :key="item.id"
                                        :label="item.type"
                                        :value="item.id">
                                        <span>${ item.type }</span>
                                    </el-option>
                                </el-select>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group req">
                                <label for="validityDate">Vence:</label>
                                <el-date-picker
                                    name="validityDate"
                                    v-model="validityDate"
                                    type="date"
                                    placeholder="Fecha de vencimiento"
                                    :picker-options="pickerOptions">
                                </el-date-picker>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button v-on:click="onSubmit" class="btn btn-success" name="button">Guardar</button>
                    <button type="button" name="button" class="btn btn-default" data-dismiss="modal" >Cancelar</button>
                </div>
            </div>
        </div>
    </div>
</div>
